{
	"name": "@diba1013/di",
	"version": "0.4.0",
	"description": "The missing dependency injection framework",
	"keywords": [
		"typescript",
		"di",
		"inject",
		"async"
	],
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/diba1013/di.git"
	},
	"author": "diba1013",
	"packageManager": "pnpm@10.27.0",
	"engines": {
		"node": ">=20",
		"pnpm": ">=10"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"require": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepare": "husky",
		"build": "tsdown",
		"build:watch": "tsdown --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"dependencies": {
		"@diba1013/types": "0.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "20.2.0",
		"@commitlint/config-conventional": "20.2.0",
		"@diba1013/linter": "0.17.1",
		"@types/node": "24.10.4",
		"@vitest/coverage-v8": "4.0.16",
		"eslint": "9.39.2",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"prettier": "3.7.4",
		"semantic-release": "25.0.2",
		"tsdown": "0.18.4",
		"typescript": "5.9.3",
		"vitest": "4.0.16"
	}
}
