{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noImplicitOverride": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true
	}
}
